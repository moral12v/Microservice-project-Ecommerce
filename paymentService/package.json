{
  "name": "order-service-for-c2b",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "build": "tsc -p tsconfig.json && cp -r src/gRPC/proto build/src/gRPC/",
    "start": "node ./build/index.js",
    "generate-schema": "ts-node src/scripts/generateSchema.ts",
    "prisma:migrate": "yarn run generate-schema && npx prisma migrate dev --schema src/prisma/schema.prisma --name init",
    "prisma:deploy": "yarn run generate-schema && npx prisma migrate deploy --schema src/prisma/schema.prisma",
    "prisma:generate": "yarn run generate-schema && npx prisma generate --schema src/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema src/prisma/schema.prisma"
  },
  "author": "VIKAS KUMAR VERMA",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "@opentelemetry/exporter-zipkin": "^1.26.0",
    "@opentelemetry/instrumentation": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@prisma/client": "^5.20.0",
    "@types/fs-extra": "^11.0.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "fs-extra": "^11.2.0",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.4.1",
    "nodemon": "^3.1.3",
    "razorpay": "^2.9.4",
    "socket.io-client": "^4.7.5",
    "ts-node": "^10.9.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/winston": "^2.4.4",
    "prisma": "^5.20.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
