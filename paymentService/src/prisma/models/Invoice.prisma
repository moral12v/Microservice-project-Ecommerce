
model Invoice {
  id                   Int       @id @default(autoincrement())
  invoiceNumber        Int
  orderId              Int
  merchantId           Int
  customerId           Int
  orderDetailId        Int
  Date                 DateTime  @default(now())
  userId               Int
  totalPayment         Int
  gst                  Int
  createdAt            DateTime  @default(now())
  updatedAt            DateTime  @updatedAt
}
